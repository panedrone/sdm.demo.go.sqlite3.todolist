<?xml version="1.0" encoding="UTF-8"?>
<dto-classes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:noNamespaceSchemaLocation="dto.xsd">

    <dto-class name="Group" ref="get_groups.sql" field-comment="`json:&quot;{column}&quot;`">
        <field type="int64 `json:&quot;g_id&quot;` // PK" column="g_id"/>
