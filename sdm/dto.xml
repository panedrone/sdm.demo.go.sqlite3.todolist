<?xml version="1.0" encoding="UTF-8"?>
<dto-classes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:noNamespaceSchemaLocation="dto.xsd">

    <dto-class name="Group" ref="get_groups.sql">
        <field type="int64 `json:&quot;g_id&quot;`" column="g_id"/>
        <field type="string `json:&quot;g_name&quot;`" column="g_name"/>
        <field type="int64 `json:&quot;tasks_count&quot;`" column="tasks_count"/>
    </dto-class>

    <dto-class name="Task" ref="tasks">
        <field type="* `json:&quot;t_id&quot;`" column="t_id"/>
        <field type="* `json:&quot;g_id&quot;`" column="g_id"/>
        <field type="* `json:&quot;t_priority&quot;`" column="t_priority"/>
        <field type="* `json:&quot;t_date&quot;`" column="t_date"/>
        <field type="* `json:&quot;t_subject&quot;`" column="t_subject"/>
        <field type="* `json:&quot;t_comments&quot;`" column="t_comments"/>
    </dto-class>

</dto-classes>
